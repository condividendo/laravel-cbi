{
    "name": "condividendo/laravel-cbi",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "ext-xmlwriter": "*",
        "orchestra/testbench": "^7.0",
        "phpstan/phpstan": "^1.8",
        "squizlabs/php_codesniffer": ">=3"
    },
    "autoload": {
        "psr-4": {
            "Condividendo\\LaravelCBI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Condividendo\\LaravelCBI\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Condividendo\\LaravelCBI\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
